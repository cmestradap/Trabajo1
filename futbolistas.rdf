</rdf:Property>

<rdf:Property rdf:ID="Tiene">
	<rdfs:domain rdf:resource="Equipo"/>
	<rdfs:range rdf:resource="Estadio"/>
</rdf:Property>

<rdf:Property rdf:ID="Entrenado_Por">
	<rdfs:domain rdf:resource="Futbolista"/>
	<rdfs:range rdf:resource="Entrenador"/>
</rdf:Property>

<rdf:Property rdf:ID="Poseedor_De">
	<rdfs:domain rdf:resource="Equipo"/>
	<rdfs:range rdf:resource="Futbolista"/>
</rdf:Property>

<rdf:Property rdf:ID="Perteneciente_a">
	<rdfs:domain rdf:resource="Futbolista"/>
	<rdfs:range rdf:resource="Equipo"/>
</rdf:Property>
	
	

