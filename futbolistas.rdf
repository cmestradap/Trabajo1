</rdf:Property>
	
<rdf:Property rdf:ID="Dirigido_Por">
	<rdfs:domain rdf:resource="Equipo"/>
	<rdfs:range rdf:resource="Entrenador"/>
</rdf:Property>

<rdf:Property rdf:ID="Sede_De">
